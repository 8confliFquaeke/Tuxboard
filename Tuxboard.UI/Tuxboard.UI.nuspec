      <files include="any/any/Views/Shared/Components/**/*.cshtml" buildAction="None" copyToOutput="true" flatten="true" />
      <files include="any/any/Views/WidgetDialog/WidgetDialog.cshtml" buildAction="None" copyToOutput="true" flatten="true" />
      <files include="any/any/Widgets/**/*.*" buildAction="None" copyToOutput="true" flatten="true" />
      <files include="any/any/wwwroot/scripts/tuxboard.js" buildAction="None" copyToOutput="true" flatten="true" />
