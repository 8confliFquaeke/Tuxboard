<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>Tuxboard.UI</id>
    <version>1.0.0</version>
    <title>Tuxboard UI</title>
    <authors>Jonathan "JD" Danylko</authors>
    <owners>Jonathan "JD" Danylko</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>UI components for Tuxboard, an open source dashboard framework for ASP.NET Core 3.1</description>
    <summary>Front-end structure and components for building your own dynamic and static dashboards</summary>
    <copyright>2019-2020</copyright>
    <dependencies>
      <dependency id="Tuxboard.Core" version="1.0.0" exclude="Build,Analyzers" />
    </dependencies>
    <contentFiles>
      <files include="contentFiles/any/any/TuxboardControllers/**/*.cs" buildAction="content" flatten="true" copyToOutput="false" />
      <files include="contentFiles/any/any/Views/LayoutDialog/LayoutColumn.cshtml"  buildAction="content" flatten="true" copyToOutput="false"/>
      <files include="contentFiles/any/any/Views/LayoutDialog/LayoutDialog.cshtml"  buildAction="content" flatten="true" copyToOutput="false"/>
      <files include="contentFiles/any/any/Views/LayoutDialog/LayoutRow.cshtml"  buildAction="content" flatten="true" copyToOutput="false"/>
      <files include="contentFiles/any/any/Views/Shared/ChangeLayoutFrame.cshtml"  buildAction="content" flatten="true" copyToOutput="false"/>
      <files include="contentFiles/any/any/Views/Shared/WidgetDialogFrame.cshtml" buildAction="content" flatten="true" copyToOutput="false" />
      <files include="contentFiles/any/any/Views/Shared/Components/**/*.cshtml" buildAction="content" flatten="true" copyToOutput="false" />
      <files include="contentFiles/any/any/Views/WidgetDialog/WidgetDialog.cshtml"  buildAction="content" flatten="true" copyToOutput="false" />
      <files include="contentFiles/any/any/Widgets/**/*.*"  buildAction="content" flatten="true" copyToOutput="false"/>
      <files include="contentFiles/any/any/wwwroot/scripts/tuxboard.js"  buildAction="content" flatten="true" copyToOutput="false"/>
      <files include="contentFiles/wwwroot/scss/**/*.*"  buildAction="content" flatten="true" copyToOutput="false"/>
    </contentFiles>
  </metadata>
  <files>
    <file src="TuxboardControllers/**/*.cs" target="/" />
    <file src="Views/LayoutDialog/LayoutColumn.cshtml" target="Views/LayoutDialog/LayoutColumn.cshtml" />
    <file src="Views/LayoutDialog/LayoutDialog.cshtml" target="Views/LayoutDialog/LayoutDialog.cshtml" />
    <file src="Views/LayoutDialog/LayoutRow.cshtml" target="Views/LayoutDialog/LayoutRow.cshtml" />
    <file src="Views/Shared/ChangeLayoutFrame.cshtml" target="Views/Shared/ChangeLayoutFrame.cshtml" />
    <file src="Views/Shared/WidgetDialogFrame.cshtml" target="Views/Shared/WidgetDialogFrame.cshtml" />
    <file src="Views/Shared/Components/**/*.cshtml" target="/" />
    <file src="Views/WidgetDialog/WidgetDialog.cshtml" target="Views/WidgetDialog/WidgetDialog.cshtml" />
    <file src="Widgets/**/*.*" target="/" />
    <file src="wwwroot/scripts/tuxboard.js" target="wwwroot/scripts/tuxboard.js" />
    <file src="wwwroot/scss/**/*.*" target="/" />
  </files>
</package>