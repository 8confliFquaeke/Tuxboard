<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>Tuxboard.UI</id>
    <version>1.0.0</version>
    <title>Tuxboard UI</title>
    <authors>Jonathan "JD" Danylko</authors>
    <owners>Jonathan "JD" Danylko</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <projectUrl>https://www.github.com/jdanylko/tuxboard/wiki</projectUrl>
    <description>UI components for Tuxboard, an open source dashboard framework for ASP.NET Core 3.1</description>
    <summary>Front-end structure and components for building your own dynamic and static dashboards</summary>
    <copyright>2019-2020</copyright>
    <dependencies>
      <dependency id="Tuxboard.Core" version="1.0.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="TuxboardControllers\**" target="content\TuxboardControllers" />
    <file src="Views\LayoutDialog\**" target="content\Views\LayoutDialog" />
    <file src="Views\Shared\Components\**" target="content\Views\Shared\Components" />
    <file src="Views\Shared\ChangeLayoutFrame.cshtml" target="content\Views\Shared\ChangeLayoutFrame.cshtml" />
    <file src="Views\Shared\WidgetDialogFrame.cshtml" target="content\Views\Shared\WidgetDialogFrame.cshtml" />
    <file src="Views\WidgetDialog\**" target="content\Views\WidgetDialog" />
    <file src="Widgets\**" target="content\Widgets" />
    <file src="wwwroot\scripts\tuxboard.js" target="content\wwwroot\scripts\tuxboard.js" />
    <file src="wwwroot\scss\**" target="content\wwwroot\scss" />
  </files>
</package>